<T3DataStructure>
  <sheets>
    <sDEF>
      <ROOT>
        <TCEforms>
          <sheetTitle>Fields</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
            
            <gallery>
                <title>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:gallery.title</title>
                <section>1</section>
                <type>array</type>
                <el>
                    <container>
                        <type>array</type>
                        <title>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:general.addnew</title>
                        <el>
                            <image>
                                <TCEforms>
                                    <label>
                                        LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:general.image
                                    </label>
                                    <config>
                                        <type>group</type>
                                        <internal_type>db</internal_type>
                                        <appearance>
                                            <elementBrowserType>file</elementBrowserType>
                                            <elementBrowserAllowed>
                                                gif,jpg,jpeg,tif,tiff,bmp,pcx,tga,png,pdf,ai,svg
                                            </elementBrowserAllowed>
                                        </appearance>
                                        <allowed>sys_file</allowed>
                                        <size>1</size>
                                        <minitems>1</minitems>
                                        <maxitems>1</maxitems>
                                        <show_thumbs>1</show_thumbs>
                                    </config>
                                </TCEforms>
                            </image>
                            <title>
                                <TCEforms>
                                    <label>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:general.year</label>
                                    <config>
                                        <type>input</type>
                                        <size>30</size>
                                        <eval>trim,required</eval>
                                    </config>
                                </TCEforms>
                            </title>
                            <subtitle>
                                <TCEforms>
                                    <label>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:general.subtitle</label>
                                    <config>
                                        <type>input</type>
                                        <size>30</size>
                                        <eval>trim</eval>
                                    </config>
                                </TCEforms>
                            </subtitle>
                            <text>
                                <TCEforms>
                                    <label>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:general.text</label>
                                    <config>
                                        <type>text</type>
                                        <rows>5</rows>
                                        <cols>30</cols>
                                        <eval>trim</eval>
                                        <enableRichtext>1</enableRichtext>
                                        <richtextConfiguration>default</richtextConfiguration>
                                    </config>
                                </TCEforms>
                            </text>
                        </el>
                    </container>
                </el>
            </gallery>
            <text>
                <TCEforms>
                    <label>LLL:EXT:ns_theme_agency/Resources/Private/Language/locallang_flex.xlf:general.bottomtext</label>
                    <config>
                        <type>text</type>
                        <rows>5</rows>
                        <cols>30</cols>
                        <eval>trim</eval>
                        <enableRichtext>1</enableRichtext>
                        <richtextConfiguration>default</richtextConfiguration>
                    </config>
                </TCEforms>
            </text>
            
        </el>
      </ROOT>
    </sDEF>
  </sheets>
</T3DataStructure>